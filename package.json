{
  "name": "wf_api",
  "version": "0.0.1",
  "description": "WF Api",
  "main": "index.js",
  "scripts": {
    "dev": "webpack --config config/webpack/dev-webpack.config.js -d --watch",
    "dev-server": "webpack-dev-server --config config/webpack/dev-webpack.config.js --inline --hot --watch --host 0.0.0.0 --port 8090",
    "start": "npm run dev-server",
    "lint": "eslint --ext .js src --ext .jsx src --ext .js test/specs",
    "prebuild": "npm run testJunit",
    "build": "webpack --config config/webpack/prod-webpack.config.js -p",
    "pretest": "npm run lint",
    "test": "mocha",
    "pretestJunit": "npm run test",
    "testJunit": "mocha --reporter mocha-junit-reporter --reporter-options mochaFile=test/test-results.xml"
  },
  "author": "Lund",
  "license": "ISC",
  "dependencies": {
    "moment": "2.18.1",
    "warframe-worldstate-data": "https://github.com/WFCD/warframe-worldstate-data/tarball/l10n"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "chai": "^3.5.0",
    "chai-immutable": "^1.6.0",
    "command-line-args": "4.0.7",
    "css-loader": "0.28.7",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.0.0",
    "eslint-plugin-html": "^2.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^2.2.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.1",
    "jsdom": "9.11.0",
    "jsdom-global": "2.1.1",
    "log-symbols": "2.0.0",
    "mocha": "^3.2.0",
    "mocha-junit-reporter": "^1.13.0",
    "node-sass": "^4.5.0",
    "postcss-loader": "^1.3.3",
    "sass-loader": "^6.0.0",
    "sinon": "^3.2.1",
    "style-loader": "^0.13.1",
    "uglify-js": "git://github.com/mishoo/UglifyJS2#harmony-v2.8.22",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-bundle-analyzer": "2.9.0",
    "webpack-dev-server": "2.3.0"
